gbif_data$species[gbif_data$species == "" | is.na(gbif_data$species)] <- "Especies desconocidas"
gbif_data$species <- gsub("^([A-Z])[a-z]+ ", "\\1. ", gbif_data$species) # para que tome solo la primer letra del género
tabla_sp <- table(gbif_data$species)
tabla_sp <- sort(tabla_sp, decreasing = TRUE)# con este código ordeno la tabla de mayor a menor
nombres_italica <- as.expression(lapply(names(tabla_sp), function(x) bquote(italic(.(x)))))
par(mar = c(10, 5, 5, 2))
colores <- c("#264653", "#2a9d8f", "#e9c46a", "#f4a261", "#e76f51")
pos<-barplot(
tabla_sp,
names.arg = nombres_italica,
las = 2,                                # Etiquetas perpendiculares
main = "Ocurrencias de Moniliophthora por especie (GBIF)",
ylab = "Número de registros",
col = colores[1:length(tabla_sp)],
border = NA,
ylim = c(0, max(tabla_sp) * 1.5),       # Alarga el eje Y un 20% más que el máximo
cex.lab = 1.1,                     # Tamaño de letra del eje Y
cex.main = 1.3                     # Tamaño del título
)
grid(nx = NA, ny = NULL, col = "gray90", lty = "solid")
text(
x = pos,
y = as.numeric(tabla_sp),
labels = as.numeric(tabla_sp),
pos = 3,             # Encima de la barra
cex = 1,           # Tamaño del número
font = 1,            # Negrita
xpd = TRUE           # Permite escribir fuera de los límites si es necesario
)
